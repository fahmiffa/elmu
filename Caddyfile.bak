mail.murika.co.id, webmail.murika.co.id {
    reverse_proxy 127.0.0.1:8080 {
        header_up Host {host}
        header_up X-Forwarded-Proto https
        header_up X-Forwarded-Ssl on
        header_up X-Url-Scheme https
        header_up X-Forwarded-For {remote_host}
    }
}


